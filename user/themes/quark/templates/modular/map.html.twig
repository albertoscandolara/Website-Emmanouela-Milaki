{% set address = page.header.address|url_encode %}
<iframe 
    width="600" 
    height="450" 
    style="border:0;" 
    loading="lazy" 
    allowfullscreen 
    referrerpolicy="no-referrer-when-downgrade"
    src="https://www.google.com/maps/embed/v1/place?key=YOUR_GOOGLE_MAPS_API_KEY&q={{ address }}">
</iframe>